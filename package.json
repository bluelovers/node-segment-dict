  "scripts": {
    "coverage": "npx nyc yarn run test",
    "docs": "npx typedoc --options ./typedoc.config.js",
    "ncu": "npx npm-check-updates -u",
    "prepack": "echo prepack",
    "postpack": "echo postpack",
    "postpublish2": "npx lerna run --scope novel-segment postpublish2 && npx lerna run --scope @novel-segment/api-server now-deploy",
    "prepublishOnly": "echo prepublishOnly & yarn run ncu & npm run sort:all && yarn run test",
    "publish": "echo publish",
    "postpublish:after": "node ./script/publish-after",
    "postpublish": "echo postpublish && yarn run postpublish:after && yarn run postpublish2",
    "sort:all": "yarn run sort:list:synonym & yarn run sort:dict:synonym & yarn run sort:line",
    "sort:dict:synonym": "ts-node ./script/sort.dict_synonym.ts",
    "sort-package-json": "npx sort-package-json ./package.json",
    "sort:list:synonym": "ts-node ./script/sort_list_synonym.ts",
    "sort:line": "ts-node ./script/sort-line.ts",
    "test": "npx lerna run --scope novel-segment test",
    "travis": "typedoc --options ./typedoc.config.js"
  },
